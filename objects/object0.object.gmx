                yy = read_ushort(read);
                if socket_id == gne_get_socket_id()
                    break;
                with(object1){
                    if(s == socket_id){
                            x = xx;
                            y = yy;
                        }
                }
                break;
            case 101:
                show_message(read_string(read))
                break;
        }
        buffer_delete(read);
}
